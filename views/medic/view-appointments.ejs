<%-include("../partials/header")%>

<h1>Your Appointments</h1>
<div class="container">
    <table class="table">
        <thead>
            <th>sno</th>
            <th>Doctor</th>
            <th>Date</th>
            <th>specialization</th>
            <th>Contact</th>
        </thead>
        <tbody>
          <%  appointment.forEach((appoint)=>{ %>
                <tr>
                    <th><%=x%></th>
                    <td><%=appoint.doctorId.name%></td>
                    <td><%=appoint.time%></td>
                    <td><%=appoint.doctorId.specialization%></td>
                    <td><%=appoint.contact%></td>
                </tr>
                <%x++;%>
           <% }) %>
           
        </tbody>
    </table>

</div>








<%-include("../partials/header")%>